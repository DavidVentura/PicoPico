function upd_menu(ev)

  if t==16 then
   music(0) 
  end 

 function cmov(n)
  sfx(7)
  sel=(sel+n)%fmax
 end
 
 f=sget(sel,6)
 fok=score>=threshold[1+f]

 if not go and t>16 then
	 if btnp(0) then cmov(-1) end
	 if btnp(1) then cmov(1) end
