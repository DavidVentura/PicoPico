  elseif fget(tl,2) then
   add(mons,{x=x,y=y,t=tl-48})
   tl=12
  end  
  if is_grey(tl) then
   obj+=1
   --tl+=64
   --tot+=1
  end

  mset(x,y,tl)
 end end 
 for m in all(mons) do
  mk_disc(m.x,m.y,m.t,true)
 end


 -- disc above
 --[[
 for e in all(ents) do
  if e.disc then
		 del(ents,e)
		 add(ents,e)
	 end
	end
	--]]
	

 -- droplet
 mk_droplet()

 -- disc sfx
 local disc_noise=function()
  local c=max(1-h.md/64,0)
	 set_vol(flr(c*c*7))
	 if t%4==0 then sfx(0) end
  if h.sliced then
   h.md+=4
  else
   h.md=128
  end	  
	 
	end
