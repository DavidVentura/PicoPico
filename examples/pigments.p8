function ysort(a)
 for i=1,#a do
  local j = i
  while j > 1 and a[j-1].y+a[j-1].ssy > a[j].y+a[j].ssy do
   a[j],a[j-1] = a[j-1],a[j]
   j = j - 1
  end
 end
end




